    "react-navigation": "^2.18.2",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-thunk": "^2.3.0",
    "seamless-immutable": "^7.1.4",
    "styled-components": "^4.0.3",
    "uuid": "^3.3.2"
